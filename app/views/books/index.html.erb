    <link rel="styesheets" href="books.scss"/>
    <h1>Books</h1>
　　<a>
　　   <div class="1">Title</div>
　  </a>
　  <a>
　　    <div class="2">Body</div>
　　</a>
　　<a>
　　  <div class="3"></div>
　　</a>
　　   <% @books.each do |book| %>
　　    <span><%= book.title %></span>
　　    <span><%= book.body %></span>
    <span><%= link_to "Show", book_path(book.id) %></span>
    <span><%= link_to "Edit",  edit_book_path(book.id) %></span>
    <span><%= link_to "Destroy", book_path(book.id), method: :delete, "data-confirm" => "Are you sure?" %></span>
    <% end %>
    <h2>New Book</h2>
    <% if @book.errors.any? %>
     <%= @book.errors.count %> errors prohibited this book from being saved:
     <ul>
       <% @book.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
     </ul>
    <% end %>

    <%= form_with model: @book do |f| %>
   <p>Title</p>
     <%= f.text_field :title %>
   <p>Body</p>
     <%= f.text_area :body %>
     <%= f.submit %>
   <% end %>
